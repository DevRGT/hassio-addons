{
  "name": "Serveo Free Reverse SSL Proxy",
  "version": "0.62",
  "slug": "serveo",
  "description": "Expose Your hassio to the internet through SSL/HTTPS with one click thanks to SERVEO.net. You DO NOT need to: have external IP, make any router configuration (no need to forward any port), create any logins (DuckDNS and others dynamic DNS services require all this hassle).",
  "url": "https://github.com/devRGT/hassio-addons/tree/master/serveo",
  "arch": ["armhf", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "host_network": true,
  "options": {
    "server": "serveo.net",
    "private_key": null,
    "domain_1": "",
    "alias_1": null,
    "ip_or_hostname_1": "localhost",
    "port1from": 8123,
    "port1to": 80,
    "domain_2": "",
    "alias_2": null,
    "ip_or_hostname_2": "localhost",
    "port2from": 0,
    "port2to": 0,
    "domain_3": "",
    "alias_3": null,
    "ip_or_hostname_3": "localhost",
    "port3from": 0,
    "port3to": 0,
    "retry_time": 15
  },
  "schema": {
    "server": "str",
    "private_key": "str",
    "domain_1": "str",
    "alias_1": "str",
    "ip_or_hostname_1": "str",
    "port1from": "int",
    "port1to": "int",
    "domain_2": "str",
    "alias_2": "str",
    "ip_or_hostname_2": "str",
    "port2from": "int",
    "port2to": "int",
    "domain_3": "str",
    "alias_3": "str",
    "ip_or_hostname_3": "str",
    "port3from": "int",
    "port3to": "int",
    "retry_time": "int"
  }
}
